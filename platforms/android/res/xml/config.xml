<?xml version='1.0' encoding='utf-8'?>
<widget id="com.afsoftwarestudios.cdinterview" version="1.0.0.147" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <allow-intent href="market:*" />
    <name>CDInterveiw</name>
    <description>
        A sample Apache Cordova based application in Android that I created for my interview process at CouponDunia.
    </description>
    <author email="aaronfurtado93@yahoo.com" href="">
        Aaron Furtado
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
